{
  "env": {
    "jest": true,
    "browser": true,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "globals": {
    "React": true
  },
  "plugins": [
    "react",
    "eslint-plugin-import"
  ],
  "rules": {
    "import/order": [
      "error",
      {
        "groups": [
          [
            "external",
            "builtin"
          ],
          [
            "parent",
            "sibling",
            "index"
          ],
          "internal"
        ]
      }
    ]
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  }
}